{
  "version": 2,
  "public": true,
  "cleanUrls": true,
  "trailingSlash": false,
  "buildCommand": "echo 'No build needed for static site'",
  "outputDirectory": "static",
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/o-nas", "destination": "/o-nas.html" },
    { "source": "/fotowoltaika", "destination": "/fotowoltaika.html" },
    { "source": "/pompy-ciepla", "destination": "/pompy-ciepla.html" },
    { "source": "/magazyny-energii", "destination": "/magazyny-energii.html" },
    { "source": "/klimatyzacja", "destination": "/klimatyzacja.html" },
    { "source": "/realizacje", "destination": "/realizacje.html" },
    { "source": "/cennik", "destination": "/cennik.html" },
    { "source": "/kontakt", "destination": "/kontakt.html" },
    { "source": "/blog", "destination": "/blog.html" },
    { "source": "/kalkulator", "destination": "/kalkulator.html" },
    { "source": "/dotacje", "destination": "/dotacje.html" },
    { "source": "/logowanie", "destination": "/logowanie.html" },
    { "source": "/panel-klienta", "destination": "/panel-klienta.html" },
    { "source": "/panel-admin", "destination": "/panel-admin.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, must-revalidate"
        }
      ]
    }
  ]
}
